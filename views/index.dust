            <div class="bar" id="breakdown"></div>   
			<div class="boxes-container" style="border: 10px black;">
                <div class="pure-g" id="trips_grid">
                </div>
            </div>
            <img class="bottom-img" src="img/tmc.png"></div>
		</div>

        

